<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="800.0" prefWidth="1550.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="graphics.MainMenu">
   <children>
      <Button fx:id="productsMenuButton" layoutX="524.0" layoutY="148.0" mnemonicParsing="false" onAction="#goToProductsMenu" prefHeight="105.0" prefWidth="502.0" text="Products Menu">
         <font>
            <Font name="Times New Roman" size="36.0" />
         </font>
      </Button>
      <Button fx:id="profileButton" layoutX="524.0" layoutY="295.0" mnemonicParsing="false" onAction="#goToProfileMenu" prefHeight="105.0" prefWidth="502.0" text="Profile">
         <font>
            <Font name="Times New Roman" size="36.0" />
         </font>
      </Button>
      <Button layoutX="687.0" layoutY="624.0" mnemonicParsing="false" onAction="#exit" prefHeight="86.0" prefWidth="176.0" text="Exit">
         <font>
            <Font name="Times New Roman" size="36.0" />
         </font>
      </Button>
      <ImageView fx:id="accountImage" fitHeight="44.0" fitWidth="45.0" layoutX="14.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../Images/account.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
